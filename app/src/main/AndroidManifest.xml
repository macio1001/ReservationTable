<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reservationtable">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_application"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RezerwacjaInfo" />
        <activity android:name=".StolikiOffActivity" />
        <activity android:name=".StolikiActivity" />
        <activity android:name=".HistoriaActivity" />
        <activity android:name=".SkanerActivity" />
        <activity android:name=".DaneosoboweActivity" />
        <activity android:name=".RezerwujActivity" />
        <activity android:name=".MenuKlientActivity" />
        <activity android:name=".HistoriaUzytkownikActivity"/>
        <activity android:name=".WyszukiwanierezerwacjiActivity"
            android:windowSoftInputMode="adjustPan|adjustResize"/>
        <activity android:name=".MenuRestauracjaActivity" />
        <activity android:name=".PrzypomnienieHaslaActivity" />
        <activity android:name=".RejestacjaActivity" />
        <activity android:name=".LogowanieActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />
    </application>

</manifest>